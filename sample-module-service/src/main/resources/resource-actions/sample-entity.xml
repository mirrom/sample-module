<?xml version="1.0"?><!DOCTYPE resource-action-mapping PUBLIC "-//Liferay//DTD Resource Action Mapping 7.3.0//EN"
        "http://www.liferay.com/dtd/liferay-resource-action-mapping_7_3_0.dtd">

<resource-action-mapping>
    
    <model-resource>
        <model-name>sample.module.model.SampleEntity</model-name>
        <portlet-ref>
            <portlet-name>
                sample_module_portlet_SampleModulePortlet
            </portlet-name>
        </portlet-ref>
        <weight>2</weight>
        <permissions>
            <supports>
                <action-key>DELETE</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>UPDATE</action-key>
                <action-key>VIEW</action-key>
            </supports>
            <site-member-defaults>
                <action-key>VIEW</action-key>
            </site-member-defaults>
            <guest-defaults>
                <action-key>VIEW</action-key>
            </guest-defaults>
            <guest-unsupported>
                <action-key>DELETE</action-key>
                <action-key>PERMISSIONS</action-key>
                <action-key>UPDATE</action-key>
            </guest-unsupported>
        </permissions>
    </model-resource>

</resource-action-mapping>
